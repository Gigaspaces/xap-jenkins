<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.plugins.disk__usage.DiskUsageProperty plugin="disk-usage@0.28"/>
    <hudson.model.ParametersDefinitionProperty>
      <parameterDefinitions>
        <hudson.model.TextParameterDefinition>
          <name>BRANCH</name>
          <description>The source branch for this xap-release</description>
          <defaultValue>master</defaultValue>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>VERSION</name>
          <description>The maven version of this xap release</description>
          <defaultValue>12.0.0-m15</defaultValue>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>TAG_NAME</name>
          <description></description>
          <defaultValue>12.0.0-m15</defaultValue>
        </hudson.model.TextParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>OVERRIDE_EXISTING_TAG</name>
          <description></description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>DEPLOY_ARTIFACTS</name>
          <description></description>
          <defaultValue>true</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.BooleanParameterDefinition>
          <name>PERFORM_FULL_M2_CLEAN</name>
          <description></description>
          <defaultValue>false</defaultValue>
        </hudson.model.BooleanParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>M2</name>
          <description></description>
          <defaultValue>/home/jenkins/.m2_release_xap</defaultValue>
        </hudson.model.TextParameterDefinition>
        <hudson.model.TextParameterDefinition>
          <name>XAP_WORKSPACE</name>
          <description></description>
          <defaultValue>/var/workspaces/release-xap</defaultValue>
        </hudson.model.TextParameterDefinition>
      </parameterDefinitions>
    </hudson.model.ParametersDefinitionProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>rm -f /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export BRANCH=$BRANCH&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export VERSION=$VERSION&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export TAG_NAME=$TAG_NAME&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export OVERRIDE_EXISTING_TAG=$OVERRIDE_EXISTING_TAG&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export DEPLOY_ARTIFACTS=$DEPLOY_ARTIFACTS&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export PERFORM_FULL_M2_CLEAN=$PERFORM_FULL_M2_CLEAN&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh
echo &quot;export WORKSPACE=$XAP_WORKSPACE&quot; &gt;&gt; /home/jenkins/xap_scripts/release-xap-setenv.sh</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>/home/jenkins/xap_scripts/release_xap.sh /home/jenkins/xap_scripts/release-xap-setenv.sh</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers/>
</project>